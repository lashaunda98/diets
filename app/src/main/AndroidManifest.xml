<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="es.victorgf87.diets" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:replace="android:icon" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WeightsActivity"
            android:label="@string/title_activity_weights"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".ExerciseActivityActivity"
            android:label="@string/title_activity_exercise" >
        </activity>
        <activity
            android:name=".ShowRecipesActivity"
            android:label="@string/title_activity_show_recipes" >
        </activity>
    </application>

</manifest>
